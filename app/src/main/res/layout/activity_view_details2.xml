<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/viewBg"    tools:context="com.obpoo.gfsfabricsoftware.Report.UI.ViewDetailsReportPO">    <include        android:id="@+id/layout"        layout="@layout/layout_action_bar"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_alignParentTop="true" />    <android.support.v4.widget.NestedScrollView        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_above="@+id/laymc"        android:layout_below="@+id/layout">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:orientation="vertical">            <android.support.v7.widget.CardView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginStart="9dp"                android:layout_marginLeft="9dp"                android:layout_marginTop="8dp"                android:layout_marginEnd="9dp"                android:layout_marginRight="9dp"                app:cardCornerRadius="0dp"                app:cardElevation="4dp">                <android.support.constraint.ConstraintLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                    <TextView                        android:id="@+id/textView42"                        android:layout_width="match_parent"                        android:layout_height="35dp"                        android:background="@color/blue_100"                        android:padding="6dp"                        android:text="Order"                        android:textColor="@android:color/white"                        android:textSize="18sp"                        app:layout_constraintEnd_toEndOf="parent"                        app:layout_constraintStart_toStartOf="parent"                        app:layout_constraintTop_toTopOf="parent" />                    <ImageView                        android:id="@+id/imageView4"                        android:layout_width="22dp"                        android:layout_height="22dp"                        android:layout_marginLeft="@dimen/padd_10"                        android:layout_marginTop="5dp"                        android:src="@drawable/factory_img"                        app:layout_constraintStart_toStartOf="parent"                        app:layout_constraintTop_toBottomOf="@+id/textView42" />                    <TextView                        android:id="@+id/factory_tv"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="@dimen/padd_10"                        android:text="N/A"                        android:textColor="@color/black"                        app:layout_constraintBottom_toBottomOf="@+id/imageView4"                        app:layout_constraintStart_toEndOf="@+id/imageView4"                        app:layout_constraintTop_toTopOf="@+id/imageView4" />                    <ImageView                        android:id="@+id/imageView6"                        android:layout_width="22dp"                        android:layout_height="22dp"                        android:layout_marginLeft="@dimen/padd_10"                        android:layout_marginTop="5dp"                        android:src="@drawable/order_img"                        app:layout_constraintStart_toStartOf="parent"                        app:layout_constraintTop_toBottomOf="@+id/imageView4" />                    <TextView                        android:id="@+id/duedate"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="@dimen/padd_10"                        android:text="N/A"                        app:layout_constraintBottom_toBottomOf="@+id/imageView6"                        app:layout_constraintStart_toEndOf="@+id/imageView6"                        app:layout_constraintTop_toTopOf="@+id/imageView6" />                    <android.support.v7.widget.CardView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginRight="@dimen/padd_10"                        app:cardBackgroundColor="@color/orange_600"                        app:cardCornerRadius="4dp"                        app:layout_constraintEnd_toEndOf="parent"                        app:layout_constraintTop_toTopOf="@id/imageView4">                        <TextView                            android:id="@+id/status"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_gravity="center"                            android:layout_marginLeft="5dp"                            android:layout_marginTop="3dp"                            android:layout_marginRight="5dp"                            android:layout_marginBottom="3dp"                            android:gravity="center"                            android:text="Pending Order"                            android:textColor="@color/white" />                    </android.support.v7.widget.CardView>                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:gravity="center"                        app:layout_constraintBottom_toBottomOf="@id/duedate"                        app:layout_constraintEnd_toEndOf="parent"                        app:layout_constraintStart_toEndOf="@id/duedate"                        app:layout_constraintTop_toTopOf="@id/duedate">                        <ImageView                            android:id="@+id/imageView7"                            android:layout_width="22dp"                            android:layout_height="22dp"                            android:src="@drawable/deliver_img" />                        <TextView                            android:id="@+id/deiverdate"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="@dimen/padd_10"                            android:text="N/A"                            app:layout_constraintBottom_toBottomOf="@+id/imageView7"                            app:layout_constraintStart_toEndOf="@+id/imageView7"                            app:layout_constraintTop_toTopOf="@+id/imageView7" />                    </LinearLayout>                    <TextView                        android:id="@+id/tv_ref"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginTop="5dp"                        android:text="refer to: "                        app:layout_constraintStart_toStartOf="@id/imageView6"                        app:layout_constraintTop_toBottomOf="@id/imageView6" />                    <TextView                        android:id="@+id/cc_email"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="@dimen/padd_10"                        android:text="rajeshnayak@gmail.com"                        android:textColor="@color/jet"                        app:layout_constraintBottom_toBottomOf="@+id/tv_ref"                        app:layout_constraintStart_toEndOf="@+id/tv_ref"                        app:layout_constraintTop_toTopOf="@+id/tv_ref" />                    <TextView                        android:id="@+id/tv_date"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginTop="5dp"                        android:text="Date:"                        app:layout_constraintStart_toStartOf="@id/tv_ref"                        app:layout_constraintTop_toBottomOf="@id/tv_ref" />                    <TextView                        android:id="@+id/create_date"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="@dimen/padd_10"                        android:text="N/A"                        android:textColor="@color/jet"                        app:layout_constraintBottom_toBottomOf="@+id/tv_date"                        app:layout_constraintStart_toEndOf="@+id/tv_date"                        app:layout_constraintTop_toTopOf="@+id/tv_date" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal"                        app:layout_constraintEnd_toEndOf="parent"                        app:layout_constraintStart_toEndOf="@+id/create_date"                        app:layout_constraintTop_toTopOf="@+id/tv_date">                        <TextView                            android:id="@+id/tv_update"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="Update:" />                        <TextView                            android:id="@+id/update_date"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="@dimen/padd_10"                            android:text="N/A"                            android:textColor="@color/jet"                            app:layout_constraintBottom_toBottomOf="@+id/tv_update"                            app:layout_constraintStart_toEndOf="@+id/tv_update"                            app:layout_constraintTop_toTopOf="@+id/tv_update" />                    </LinearLayout>                    <ImageView                        android:id="@+id/div"                        android:layout_width="match_parent"                        android:layout_height="1dp"                        android:layout_marginLeft="20dp"                        android:layout_marginTop="5dp"                        android:layout_marginRight="20dp"                        android:src="@color/placeholder_bg"                        app:layout_constraintEnd_toEndOf="parent"                        app:layout_constraintStart_toStartOf="parent"                        app:layout_constraintTop_toBottomOf="@+id/tv_date" />                    <ImageView                        android:id="@+id/imageView8"                        android:layout_width="22dp"                        android:layout_height="22dp"                        android:layout_marginLeft="@dimen/padd_10"                        android:layout_marginTop="5dp"                        android:src="@drawable/person_img"                        app:layout_constraintStart_toStartOf="parent"                        app:layout_constraintTop_toBottomOf="@+id/div" />                    <TextView                        android:id="@+id/stafname"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="@dimen/padd_10"                        android:text="N/A"                        android:textColor="@color/black"                        app:layout_constraintBottom_toBottomOf="@+id/imageView8"                        app:layout_constraintStart_toEndOf="@+id/imageView8"                        app:layout_constraintTop_toTopOf="@+id/imageView8" />                    <ImageView                        android:id="@+id/imageView9"                        android:layout_width="22dp"                        android:layout_height="22dp"                        android:layout_marginLeft="@dimen/padd_10"                        android:layout_marginTop="5dp"                        android:layout_marginBottom="8dp"                        android:src="@drawable/mail_logo"                        app:layout_constraintBottom_toBottomOf="parent"                        app:layout_constraintStart_toStartOf="parent"                        app:layout_constraintTop_toBottomOf="@+id/imageView8" />                    <TextView                        android:id="@+id/fac_email"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="@dimen/padd_10"                        android:text="N/A"                        android:textColor="@color/blue_100"                        app:layout_constraintBottom_toBottomOf="@+id/imageView9"                        app:layout_constraintStart_toEndOf="@+id/imageView9"                        app:layout_constraintTop_toTopOf="@+id/imageView9" />                </android.support.constraint.ConstraintLayout>            </android.support.v7.widget.CardView>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginStart="9dp"                android:layout_marginLeft="9dp"                android:layout_marginTop="15dp"                android:layout_marginEnd="9dp"                android:layout_marginRight="9dp"                android:orientation="vertical">                <TextView                    android:id="@+id/textView43"                    android:layout_width="match_parent"                    android:layout_height="35dp"                    android:background="@color/blue_100"                    android:padding="6dp"                    android:text="Requested Items"                    android:textColor="@android:color/white"                    android:textSize="18sp" />                <android.support.v7.widget.RecyclerView                    android:id="@+id/recycler_view"                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                </android.support.v7.widget.RecyclerView>            </LinearLayout>        </LinearLayout>    </android.support.v4.widget.NestedScrollView></RelativeLayout>