<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/viewBg"    tools:context="com.obpoo.gfsfabricsoftware.Report.UI.FilterViewReport">    <include        android:id="@+id/layout"        layout="@layout/layout_action_bar"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_alignParentTop="true" />    <TextView        android:id="@+id/status_tv"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/layout"        android:layout_marginTop="15dp"        android:layout_marginLeft="@dimen/padd_10"        android:text="Choose Status:" />    <TextView        android:id="@+id/status_et"        android:layout_width="match_parent"        android:layout_height="35dp"        android:layout_below="@+id/status_tv"        android:layout_marginLeft="@dimen/padd_10"        android:layout_marginRight="@dimen/padd_10"        android:paddingLeft="@dimen/padd_10"        android:textColor="@color/black"        android:background="@drawable/rounded_edittx_bck" />    <TextView        android:id="@+id/from"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/status_et"        android:layout_marginLeft="@dimen/padd_10"        android:layout_marginTop="@dimen/padd_10"        android:text="From"        android:textColor="@color/black" />    <TextView        android:id="@+id/from_date"        android:layout_width="wrap_content"        android:layout_height="35dp"        android:layout_below="@+id/from"        android:layout_alignParentStart="true"        android:layout_marginLeft="@dimen/padd_10"        android:layout_marginRight="5dp"        android:paddingLeft="@dimen/padd_10"        android:layout_toStartOf="@+id/divH"        android:textColor="@color/black"        android:background="@drawable/rounded_edittx_bck" />    <View        android:id="@+id/divH"        android:layout_width="1dp"        android:layout_height="wrap_content"        android:layout_centerHorizontal="true" />    <TextView        android:id="@+id/to_date"        android:layout_width="wrap_content"        android:layout_height="35dp"        android:layout_below="@+id/from"        android:layout_alignParentEnd="true"        android:layout_marginLeft="5dp"        android:layout_marginRight="@dimen/padd_10"        android:paddingLeft="@dimen/padd_10"        android:layout_toEndOf="@+id/divH"        android:textColor="@color/black"        android:background="@drawable/rounded_edittx_bck" />    <TextView        android:id="@+id/to"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/status_et"        android:layout_marginLeft="@dimen/padd_10"        android:layout_marginTop="@dimen/padd_10"        android:layout_toEndOf="@+id/divH"        android:text="To"        android:textColor="@color/black" />    <android.support.v7.widget.CardView        android:id="@+id/conf"        android:layout_width="120dp"        android:layout_height="35dp"        android:layout_below="@+id/from_date"        android:layout_centerHorizontal="true"        android:layout_marginTop="15dp"        app:cardBackgroundColor="@color/green_100"        app:cardCornerRadius="4dp"        app:cardElevation="4dp">        <TextView            android:id="@+id/confirm"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:gravity="center"            android:text="Process"            android:textColor="@color/white" />    </android.support.v7.widget.CardView></RelativeLayout>